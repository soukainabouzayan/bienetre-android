<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bienetre">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bienetre">
        <activity
            android:name=".HomeActivity"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HandMaskActivity"
            android:exported="true"
            android:parentActivityName=".MaskActivity" />
        <activity
            android:name=".HairMaskActivity"
            android:exported="true"
            android:parentActivityName=".MaskActivity" />
        <activity
            android:name=".BodyMaskActivity"
            android:exported="true"
            android:parentActivityName=".MaskActivity" />
        <activity
            android:name=".FaceMaskActivity"
            android:exported="true"
            android:parentActivityName=".MaskActivity" />
        <activity
            android:name=".MaskActivity"
            android:exported="true"
            android:parentActivityName=".HomeActivity">
        </activity>
    </application>

</manifest>